{
  "name": "apidoc-plugin-ts",
  "version": "0.1.2",
  "description": "A plugin for apidoc leveraging TypeScript interfaces.",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "apidoc": "apidoc -i test/ -o test/out",
    "copy:plugin": "rm -rf node_modules/apidoc-plugin-ts && copyfiles package.json './dist/**/*' node_modules/apidoc-plugin-ts/",
    "lint": "tslint --type-check --project tsconfig.json",
    "test": "mocha -r ts-node/register test/**/test.ts",
    "prepare": "rm -rf dist && tsc -p src/tsconfig.json",
    "prepublish": "npm run prepare && npm run lint && npm run test",
    "pretest": "rm -rf test/out && npm run copy:plugin && npm run apidoc"
  },
  "author": "Tom Grey",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "bugs": {
    "url": "https://github.com/tgreyuk/apidoc-plugin-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/apidoc-plugin-ts.git"
  },
  "keywords": [
    "apidoc",
    "apidoc plugin",
    "typescript"
  ],
  "peerDependencies": {
    "apidoc": "^0.17.6"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.10",
    "apidoc": "^0.17.6",
    "chai": "^4.1.0",
    "copyfiles": "^1.2.0",
    "mocha": "^4.0.1",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0"
  },
  "dependencies": {
    "ts-simple-ast": "0.48.0",
    "typescript": "2.4.1"
  },
  "greenkeeper": {
    "ignore": [
      "ts-simple-ast",
      "typescript"
    ]
  }
}
